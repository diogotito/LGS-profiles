    <script>local FREELOOK_DELAY = 30

function OnEvent(event, arg)
	OutputLogMessage("event = %s, arg = %s\n", event, arg);
	
	--------------------------------------------------------
	-- Freelook mode
	--------------------------------------------------------

	if arg == 6 then
		if event == "MOUSE_BUTTON_PRESSED" then
			PressKey("lalt")
			Sleep(FREELOOK_DELAY)
			PressMouseButton(1)
		elseif event == "MOUSE_BUTTON_RELEASED" then
			ReleaseKey("lalt")
			ReleaseMouseButton(1)
		end
	end

	if arg == 3 then
		if event == "MOUSE_BUTTON_PRESSED" then
			PressKey("lalt")
			Sleep(FREELOOK_DELAY)
			PressMouseButton(3)
		elseif event == "MOUSE_BUTTON_RELEASED" then
			ReleaseKey("lalt")
			ReleaseMouseButton(3)
		end
	end
end</script>
